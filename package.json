{
    "name": "spine-animation-preview",
    "description": "spine animation preview for vscode",
    "version": "0.0.2",
    "publisher": "neymanushka",
    "private": true,
    "license": "MIT",
    "engines": {
        "vscode": "^1.47.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/neymanushka/spine_preview_ext"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCustomEditor:spinePreview.preview"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "customEditors": [
            {
                "viewType": "spinePreview.preview",
                "displayName": "Spine preview",
                "selector": [
                    {
                        "filenamePattern": "*.atlas"
                    }
                ]
            }
        ]
    },
    "scripts": {
        "pack": "vsce package",
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint . --ext .ts,.tsx",
        "watch": "tsc -w -p ./"
    },
    "devDependencies": {
        "vsce": "^2.9.1",
        "@types/node": "^16.11.7",
        "@types/vscode": "^1.47.0",
        "@types/vscode-webview": "^1.57.0",
        "@typescript-eslint/eslint-plugin": "^5.19.0",
        "@typescript-eslint/parser": "^5.19.0",
        "eslint": "^8.13.0",
        "typescript": "^4.7.2"
    }
}
